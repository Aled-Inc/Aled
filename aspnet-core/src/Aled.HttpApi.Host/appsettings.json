{
  "App": {
    "CorsOrigins": "{{App.CorsOrigins}}",
    "ClientUrl": "{{App.ClientUrl}}"
  },
  "ConnectionStrings": {
    "Default": "{{ConnectionStrings.Default}}"
  },
  "Redis": {
    "Configuration": "{{Redis.Configuration}}"
  },
  "AuthServer": {
    "Authority": "{{AuthServer.Authority}}",
    "RequireHttpsMetadata": "{{AuthServer.RequireHttpsMetadata}}",
    "SwaggerClientId": "{{AuthServer.SwaggerClientId}}"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "{{StringEncryption.DefaultPassPhrase}}"
  },
  "RemoteServices": {
    "Default": {
      "BaseUrl": "{{RemoteServices.Default.BaseUrl}}"
    },
    "AledOpenFoodFactService": {
      "BaseUrl": "{{RemoteServices.AledOpenFoodFactService.BaseUrl}}"
    }
  },
  "JwtSettings": {
    "ValidIssuer": "{{JwtSettings.ValidIssuer}}",
    "ValidAudience": "{{JwtSettings.ValidAudience}}"
  },
  "IdentityClients": {
    "AledOpenFoodFactService": {
      "GrantType": "{{IdentityClients.AledOpenFoodFactService.GrantType}}",
      "ClientId": "{{IdentityClients.AledOpenFoodFactService.ClientId}}",
      "UserName": "{{IdentityClients.AledOpenFoodFactService.UserName}}",
      "UserPassword": "{{IdentityClients.AledOpenFoodFactService.UserPassword}}",
      "Authority": "{{IdentityClients.AledOpenFoodFactService.Authority}}",
      "Scope": "{{IdentityClients.AledOpenFoodFactService.Scope}}"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "{{Kestrel.Endpoints.Https.Url}}",
        "Certificate": {
          "Path": "{{Kestrel.Endpoints.Https.Certificate.Path}}",
          "Password": "{{Kestrel.Endpoints.Https.Certificate.Password}}"
        }
      }
    }
  },
  "Settings": {
    "Abp.Mailing.Smtp.Host": "{{Settings.Abp.Mailing.Smtp.Host}}",
    "Abp.Mailing.Smtp.Port": "{{Settings.Abp.Mailing.Smtp.Port}}",
    "Abp.Mailing.Smtp.UserName": "{{Settings.Abp.Mailing.Smtp.UserName}}",
    "Abp.Mailing.Smtp.Password": "{{Settings.Abp.Mailing.Smtp.Password}}",
    "Abp.Mailing.Smtp.Domain": "{{Settings.Abp.Mailing.Smtp.Domain}}",
    "Abp.Mailing.Smtp.EnableSsl": "{{Settings.Abp.Mailing.Smtp.EnableSsl}}",
    "Abp.Mailing.Smtp.UseDefaultCredentials": "{{Settings.Abp.Mailing.Smtp.UseDefaultCredentials}}",
    "Abp.Mailing.DefaultFromAddress": "{{Settings.Abp.Mailing.DefaultFromAddress}}",
    "Abp.Mailing.DefaultFromDisplayName": "{{Settings.Abp.Mailing.DefaultFromDisplayName}}"
  },
  "Email": {
    "Default": "{{Email.Default}}"
  },
  "Ngrok": {
    "Api.Host.Url": "{{Ngrok.Api.Host.Url}}"
  }
}
